y=input("введите ваше ФИО \n")
x=input("введите содержание витамина D \n")
i=input("введите содержание йода \n")
x=int(x)
i=int(i)

if x>30:
    print("Уважааемый", y)
    print("Ваш показатель витамина D", x ,"в норме")
    if x==30:
        print("Уважааемый", y)
        print("Ваш показатель витамина D", x ,"в норме")
else:
    print("Уважааемый", y)
    print("Ваш показатель витамина D", x ,"низкий")
if i>30:
    print("Уважааемый", y)
    print("Ваш показаль йода", i ,"в норме")
    if i==30:
        print("Уважааемый", y)
        print("Ваш показаль йода", i ,"в норме")
else:
    print("Уважааемый", y)
    print("Ваш показатель йода", i ,"низкий")
