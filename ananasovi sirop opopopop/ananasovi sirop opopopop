probeg = input("ведите пробег сейчас \n" )
probeg=int(probeg)
f = open('data.txt', 'r', encoding="utf-8")
str1 = f.readline()
str2 = f.readline()
str3 = f.readline()
str4 = f.readline()
str1=int(str1)
str2=int(str2)
str3=int(str3)
str4=int(str4)
n=(probeg-str1)
print(n)
f.close()
g=(str1+n)
t=(str2+n)
r=(str3+n)
e=(str4+n)
g=str(g)
t=str(t)
r=str(r)
e=str(e)
f = open('data2.txt', 'w', encoding="utf-8")
f.write(g)
