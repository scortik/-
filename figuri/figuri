size(600,600)
figuri=[ "круг","квадрат","kvadrat","kryg","kryg","kryg","круг"]
for x in range(len(figuri)):
    if figuri[x] == "круг" :
        ellipse(50,50,50,50)
    if figuri[x] == "квадрат":
        rect(100,100,150,100)
        translate(0,100)
    if figuri[x] == "kvadrat":
        rect(200,200,300,200)
    if figuri[x] == "kryg":
        ellipse(30,0,20,20)
        translate(0,50)
