def setup():
    size(600,600) 
def draw():
    background(0,0,0)
    frameRate(2)
    push()
    fill(0,120,0)
    noStroke()
    translate(300,300)
    for step in range(4):
        triangle(60,0,-60,0,0,-60)
        translate(0,60)
    pop()
    #push()
    #rect(30,20,60,40)
    #rotate(radians
    for step in range(30):
        strokeWeight(random(0,20))
        stroke(255,255,255)
        point(random(0,600),random(0,600))
