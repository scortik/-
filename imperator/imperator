x=0
def setup():
    size(600,600)
def draw():
    background(255,0,0)
    global x
    translate(200,200)
    rotate(radians(x))
    x = x +1
    for x in range(20):
        rotate(radians(20))
        push()
        for y in range(20):
            translate(0,30)
            ellipse(30,20,30,40)
        pop()
